- if user_signed_in?
  %li= link_to 'Edit account', edit_user_registration_path
  %li= link_to 'Sign out', destroy_user_session_path, :method=>'delete'
- else
  %li= link_to 'Login', new_user_session_path, class: 'button'
  %li= link_to 'Register', new_user_registration_path, class: 'button button--highlight'
- if user_signed_in?
  - if current_user.try(:admin?)
    %li= link_to 'Users', users_path
